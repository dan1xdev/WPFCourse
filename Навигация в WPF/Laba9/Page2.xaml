<Page x:Class="MultiPageForm.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Контактные данные">
    <StackPanel Margin="20" Width="300">
        <Label Content="Email:"/>
        <TextBox x:Name="EmailTextBox"/>

        <Label Content="Телефон:" Margin="0,10,0,0"/>
        <TextBox x:Name="PhoneTextBox"/>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,20,0,0">
            <Button Content="Назад" Margin="0,0,10,0" Click="BackButton_Click"/>
            <Button Content="Далее" Click="NextButton_Click"/>
        </StackPanel>
    </StackPanel>
</Page>